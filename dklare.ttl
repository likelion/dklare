@prefix  rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix    d: <http://dklare.org/2021/10/dklare#> .

d:arg
  a rdf:Property ;
  rdfs:domain d:Lambda .

d:exp
  a rdf:Property ;
  rdfs:domain d:Lambda .

d:con
  a rdf:Property ;
  rdfs:domain d:Lambda .

d:define
  a rdf:Property ;
  rdfs:domain d:Function .

d:functor
  a rdf:Property ;
  rdfs:domain d:Function .

d:rdf
  a rdf:Property ;
  rdfs:domain d:Pattern .

d:rdfs
  a rdf:Property ;
  rdfs:domain d:Pattern .

d:not
  a rdf:Property ;
  rdfs:domain d:Pattern .

d:optional
  a rdf:Property ;
  rdfs:domain d:Pattern .

d:id
  a rdf:Property ;
  rdfs:domain d:Pattern .
